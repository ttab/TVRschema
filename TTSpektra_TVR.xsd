<!-- ! Rootstructure -->
	<xsd:element name="TTSTVR">
		<xsd:annotation><xsd:documentation>Root-element in TT TV-radio xml. Can contain one or more TVRProgramBroadcast which old the individual broadcasts.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="TVRProgramBroadcast"  maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="id" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>ID for this whole table</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="RelatedTo" type="xsd:integer" use="optional"><xsd:annotation><xsd:documentation>The latest update to this table was in realtion from this imported table.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="PendingRelatedTo" type="xsd:integer" use="optional"><xsd:annotation><xsd:documentation>ID of incoming table that this data is being updated against.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="InitialRelatedTo" type="xsd:integer" use="optional"><xsd:annotation><xsd:documentation>ID of the first imported table that this was built from. So the editor can back to the original.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="CreatedDate" type="xsd:dateTime" use="optional" ><xsd:annotation><xsd:documentation>Date and time when this table was created.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="ChangedDate" type="xsd:dateTime" use="optional" ><xsd:annotation><xsd:documentation>Date and time when this table was updated.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="ChangedBy" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Updated by.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="CreatedBy" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Created by.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="WorkStatus" type="xsd:nonNegativeInteger" use="optional"><xsd:annotation><xsd:documentation>Status of this table.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="HoldingProgramsStartingAtOrAfter" type="xsd:dateTime" use="optional"><xsd:annotation><xsd:documentation>Starttime for the so called table-day. Not the same as calendar-day.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="HoldingProgramsStartingBefore" type="xsd:dateTime" use="optional"><xsd:annotation><xsd:documentation>Endtime for the so called table-day. Not the same as calendar-day.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="LastMadeExportableDate" type="xsd:dateTime" use="optional" ><xsd:annotation><xsd:documentation>Date and time this table was ready for export.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal operations.</xsd:documentation></xsd:annotation></xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- Rootstructure -->

	
<!--   Structures for the main building blocks of programs -->
	
	<!--  TVRProgramBroadcast -->
	<xsd:element name="TVRProgramBroadcast">
	<xsd:complexType>
		<xsd:annotation><xsd:documentation>This hold one program. It has one part for the broadcastinformation and one part for the more stable programinformation.</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="TVRBroadcast" minOccurs="1" maxOccurs="1"/>
			<xsd:element ref="TVRProgram"  minOccurs="1" maxOccurs="1"/>
			<xsd:element  name="ProgramMatches" minOccurs="0" maxOccurs="1">
				<xsd:annotation><xsd:documentation>List of programs from the registry that match this program</xsd:documentation></xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MatchedProgram" minOccurs="1" maxOccurs="unbounded">
							<xsd:annotation><xsd:documentation>One program that matches this program.</xsd:documentation></xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element ref="TVRBroadcast" minOccurs="0" maxOccurs="1"/>
									<xsd:element ref="TVRProgram"  minOccurs="1" maxOccurs="1"/>
								</xsd:sequence>
								<xsd:attribute name="ProgramIdReference" type="xsd:string"><xsd:annotation><xsd:documentation>Pointer to the program that matches.</xsd:documentation></xsd:annotation></xsd:attribute>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>	
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal operation</xsd:documentation></xsd:annotation></xsd:attribute>
	</xsd:complexType>
	</xsd:element>
	<!-- TVRProgramBroadcast -->

	
<!--      Broadcast-part -->
	
	<!--  TVRBroadcast -->
	<xsd:element name="TVRBroadcast">
		<xsd:complexType>
			<xsd:annotation><xsd:documentation>All information about the current broadcast of this program</xsd:documentation></xsd:annotation>
			<xsd:sequence>
				
				<xsd:element ref="Channel"><xsd:annotation><xsd:documentation>Name and id of the channel where it is broadcasted.</xsd:documentation></xsd:annotation></xsd:element>
				
				<xsd:element name="BroadcastDateTime">
					<xsd:complexType>
						<xsd:annotation><xsd:documentation>Exact date and time for broadcast-.</xsd:documentation></xsd:annotation>
						<xsd:sequence>
							<xsd:element name="StartDateTime" minOccurs="1" maxOccurs="1"><xsd:annotation><xsd:documentation>Startdate and possibly starttime of this broadcast.</xsd:documentation></xsd:annotation>
								<xsd:simpleType>
									<xsd:union memberTypes="xsd:dateTime xsd:date"/>
								</xsd:simpleType>	
							</xsd:element>
							<xsd:element name="EndDateTime" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>End date and possibly end time of this broadcast.</xsd:documentation></xsd:annotation>
								<xsd:simpleType>
									<xsd:union memberTypes="xsd:dateTime xsd:date"/>
								</xsd:simpleType>	
							</xsd:element>
							<xsd:element name="BroadcastSlotLength" type="xsd:integer" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>The length of the broadcast in seconds.</xsd:documentation></xsd:annotation></xsd:element>
							<xsd:element name="BroadcastDuration" type="xsd:integer" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>The net length in minutes of the content in the way it is broadcasted in this occasion.</xsd:documentation></xsd:annotation></xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
					
				<xsd:element ref="BroadcastInformation" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Information about this broadcast.</xsd:documentation></xsd:annotation></xsd:element>
				
				<xsd:element ref="OtherBroadcast"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Information about reruns that already took place or will take place in the future.</xsd:documentation></xsd:annotation></xsd:element>
			
				<xsd:element name="ShowviewCode" type="xsd:string" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Showviewcode for this broadcast in this channel.</xsd:documentation></xsd:annotation></xsd:element>
			</xsd:sequence>

			<xsd:attribute name="SectionBroadcast" type="BroadcastTypeEnumList" use="optional"><xsd:annotation><xsd:documentation>SectionBroadcast is normally None, or even missing. Otherwise it can be Block or Section.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID för internal operation.</xsd:documentation></xsd:annotation></xsd:attribute>
			
		</xsd:complexType>
	</xsd:element>	
	<!-- TVRBroadcast -->
	

	<!--  BroadcastInformation -->
	<xsd:element name="BroadcastInformation">
		<xsd:annotation><xsd:documentation>Information about this broadcast.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="TexttvInfoPage" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation><xsd:documentation>If there is information about this broadcast on text-tv. Pagenumber in element content.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="LanguageCode" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Language code for the text-tv page. </xsd:documentation></xsd:annotation></xsd:attribute>
								<xsd:attribute name="Type" type="xsd:string"><xsd:annotation><xsd:documentation>If the text-tv page is of a special kind. </xsd:documentation></xsd:annotation></xsd:attribute>
							</xsd:extension>	
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				
				<xsd:element name="WebPage" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation><xsd:documentation>Web-page about this broadcast</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="URL" type="xsd:string"><xsd:annotation><xsd:documentation>The URL to the web page. </xsd:documentation></xsd:annotation></xsd:attribute>
							</xsd:extension>	
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="AudioLanguage"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Languages being used in the program in audio.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="Subtitles"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Language that can be read from subtitles.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="SignLanguage" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
							<xsd:attribute name="Value" type="xsd:boolean" default="false"><xsd:annotation><xsd:documentation>Set to true if there is an option to see sign language.</xsd:documentation></xsd:annotation></xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="TexttvSubtitles"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Language and page number if you can activate subtitles from text-tv.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="TechnicalProperty" type="TechnicalPropertyEnumList" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Technical information from a list of possible values.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="AdditionalMedia" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation><xsd:documentation>Om sändningen även sker på annat sätt än den normala</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="Type" type="AdditionalMediaTypeEnumList" ><xsd:annotation><xsd:documentation>What type of broadcast it is. </xsd:documentation></xsd:annotation></xsd:attribute>
								<xsd:attribute name="URL" type="xsd:string"><xsd:annotation><xsd:documentation>URL if the broadcast can be received from the web. </xsd:documentation></xsd:annotation></xsd:attribute>
							</xsd:extension>	
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SeriesEvent" type="SeriesEventTypeEnumList" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>If it is an episode in a series with some special note, like season last.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="BroadcastEvent" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>If there is something special with this broadcast, like for example Premiere.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="Type" type="BroadcastEventTypeEnumList"></xsd:attribute>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ChannelPrio" type="xsd:string" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>If the channel has set a priority for this broadcast.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="TTSPrio" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>If TT has set a priority for this broadcast.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:attribute name="Value" type="xsd:nonNegativeInteger" use="required"><xsd:annotation><xsd:documentation>0 isfalse 1 is true.</xsd:documentation></xsd:annotation></xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="BroadcastInfo" type="xsd:string"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Extra information that has no other place.</xsd:documentation></xsd:annotation></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>	
	<!-- BroadcastInformation -->
	
	
	<!--  OtherBroadcast -->
	<xsd:element name="OtherBroadcast">
		<xsd:annotation><xsd:documentation>Information about past or coming reruns of this program.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Direction" type="DirectionEnumList"  minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Tidigare mean it has been run previously, Kommande mean it will be rerun in the future.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="OtherBroadcastDateTime" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Date and time for the referenced rerun.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="StartDateTime" minOccurs="0" maxOccurs="1">
								<xsd:annotation><xsd:documentation>Start datum och tid för en other broadcast.</xsd:documentation></xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="Year" type="xsd:string"><xsd:annotation><xsd:documentation>Year, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Month" type="xsd:string"><xsd:annotation><xsd:documentation>Month, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Day" type="xsd:string"><xsd:annotation><xsd:documentation>Day, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Hour" type="xsd:string"><xsd:annotation><xsd:documentation>Hour, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Minute" type="xsd:string"><xsd:annotation><xsd:documentation>Minut, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Second" type="xsd:string"><xsd:annotation><xsd:documentation>Second, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Timezone" type="xsd:string"><xsd:annotation><xsd:documentation>Time difference. If empty UTC.</xsd:documentation></xsd:annotation></xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="EndDateTime" minOccurs="0" maxOccurs="1">
								<xsd:annotation><xsd:documentation>Slut datum och tid för en other broadcast.</xsd:documentation></xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="Year" type="xsd:string"><xsd:annotation><xsd:documentation>Year, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Month" type="xsd:string"><xsd:annotation><xsd:documentation>Month, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Day" type="xsd:string"><xsd:annotation><xsd:documentation>Day, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Hour" type="xsd:string"><xsd:annotation><xsd:documentation>Hour, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Minute" type="xsd:string"><xsd:annotation><xsd:documentation>Minut, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Second" type="xsd:string"><xsd:annotation><xsd:documentation>Second, if known</xsd:documentation></xsd:annotation></xsd:attribute>
									<xsd:attribute name="Timezone" type="xsd:string"><xsd:annotation><xsd:documentation>Time difference. If empty UTC.</xsd:documentation></xsd:annotation></xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="OtherBroadcastDescription" type="xsd:string" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Simple textual information about this rerun. Sometimes this is the only information.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="Channel" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Channel of other broadcast. If missing it is the same channel as this broadcast-</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="AudioLanguage"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Language heard in the other broadcast.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="Subtitles"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Language used on subtitles in other broadcast.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="TexttvSubtitles" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Pages on text-tv subtitles for other broadcast.</xsd:documentation></xsd:annotation></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>		
	<!-- OtherBroadcast -->
	

	
<!--       Programinformation -struktur -->
	
	<!--  TVRProgram -->
	<xsd:element name="TVRProgram">
		<xsd:annotation><xsd:documentation>Information about a program. Information that is valid regardless of broadcasting occasion.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				
				<xsd:element name="MediaType" type="MediaTypeEnumList"><xsd:annotation><xsd:documentation>If the program is radio or tv.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="ProgramFormat" type="ProgramFormatEnumList" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Type of program</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="ChannelItemID" type="xsd:string" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Channel specific id for the program information.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ChannelRegistryReferenceID"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Channel registry id for this program.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="OriginalProgramTitle" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Title of program in original language.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="EpisodeNumber" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Episode number and which channel has set it.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:positiveInteger">
								<xsd:attribute name="Scope" type="ScopeEnumList" use="required"><xsd:annotation><xsd:documentation>In what timeframe the numbering is, per season or totally.</xsd:documentation></xsd:annotation></xsd:attribute>
								<xsd:attribute name="AssignedBy" type="xsd:string"><xsd:annotation><xsd:documentation>Who has set this episode number.</xsd:documentation></xsd:annotation></xsd:attribute>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>				
				<xsd:element ref="Title"  minOccurs="1" maxOccurs="unbounded"></xsd:element>
				<xsd:element ref="ChannelAgeLimit" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Information about age restrictions. Connected with specific channels.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="VersionableInfo" minOccurs="1" maxOccurs="1">
					<xsd:annotation><xsd:documentation>Versions of textual information about the program.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Version" minOccurs="1" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>One version of textual information.</xsd:documentation></xsd:annotation></xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!--<xsd:element name="Image" type="ImageType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>DEPRECATED till förmån för MediaItem</xsd:documentation></xsd:annotation></xsd:element>-->
				<xsd:element ref="MediaItems" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>MediaItems can be any media objects but normally images.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="Description" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Text descriptions</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="PersonList" type="PersonListType" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>List of persons involved in the program. </xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ExtraInfo" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Extra information that has no other place.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ProgramData" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Data about the program.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="SeriesSeason" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Information about the season of a series.</xsd:documentation></xsd:annotation> </xsd:element>

				<xsd:element ref="Reviews" minOccurs="0" maxOccurs="1"></xsd:element>
    
			</xsd:sequence>
			
			<xsd:attributeGroup ref="grundAttributGrupp"/>
			<xsd:attribute name="MatchRank" type="xsd:integer"><xsd:annotation><xsd:documentation>Ranking value in this match.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="MadeExportableNumberOfTimes" type="xsd:integer"><xsd:annotation><xsd:documentation>Number of times this program has been made exportable</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="CreatedFromInputItemID" type="xsd:integer"><xsd:annotation><xsd:documentation>What input this program was created from</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="ExportableVersionId" type="xsd:integer" use="optional"><xsd:annotation><xsd:documentation>ID for the exportable version. To make sure the program has the same id even if exported several times.</xsd:documentation></xsd:annotation></xsd:attribute>
		</xsd:complexType>
	</xsd:element>	
	<!-- TVRProgram -->
	
<!-- Season information -->	
		
	<!--  SeriesSeason -->
	<xsd:element name="SeriesSeason">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ProgramFormat" type="ProgramFormatEnumList" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>If the program is radio or tv.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="SeasonYear" type="xsd:string" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Year, or years, this season is from.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="SeasonLabel" type="xsd:string" minOccurs="0"><xsd:annotation><xsd:documentation>Information about the season.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="OriginalSeasonTitle" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Original title of the season.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ChannelRegistryReferenceID"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
				<xsd:element name="TotalEpisodesInSeason" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation><xsd:documentation>Number of episodes in this series this season. Can be different in different channels.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:nonNegativeInteger">
								<xsd:attribute name="AssignedBy" type="xsd:string"></xsd:attribute>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
					
				</xsd:element>
				<xsd:element ref="Title"  minOccurs="1" maxOccurs="unbounded"></xsd:element>
				<xsd:element ref="ChannelAgeLimit" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Age limit information.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="VersionableInfo" minOccurs="0" maxOccurs="1">
					<xsd:annotation><xsd:documentation>Versions of textual information about this season.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Version" minOccurs="1" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>One version of textual information about this season.</xsd:documentation></xsd:annotation></xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
<!--				<xsd:element name="Image" type="ImageType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>DEPRECATED till förmån för MediaItem</xsd:documentation></xsd:annotation></xsd:element>-->
				<xsd:element ref="MediaItems" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Media objects connected with this season.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="Description" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Text descriptions of this season.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="PersonList" type="PersonListType" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Persons and their functions for this season.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ExtraInfo" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Extra information about the season.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ProgramData" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Data that is valid for all episodes of this season.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ProgramSeries" minOccurs="1" maxOccurs="1"><xsd:annotation><xsd:documentation>Information about the serie that this season is part of.</xsd:documentation></xsd:annotation></xsd:element>
			</xsd:sequence>
			<xsd:attributeGroup ref="grundAttributGrupp"/>
			<xsd:attribute name="MatchRank" type="xsd:integer"><xsd:annotation><xsd:documentation>Ranking in the registry matching</xsd:documentation></xsd:annotation></xsd:attribute>
			
		</xsd:complexType>
	</xsd:element>
	<!-- SeriesSeason -->
	
<!--  Basic information about a serie -->	
	
	<!--  ProgramSeries -->
	<xsd:element name="ProgramSeries">
		<xsd:annotation><xsd:documentation>Information about a serie that is valid for all seasons and all episodes.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ProgramFormat" type="ProgramFormatEnumList" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>If it is radio or tv.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="OriginalSeriesTitle" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Original title of the series.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ChannelRegistryReferenceID"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
				<xsd:element name="TotalEpisodesInSeries" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation><xsd:documentation>Total number of episodes in this serie.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:nonNegativeInteger">
								<xsd:attribute name="AssignedBy" type="xsd:string"></xsd:attribute>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
					
				</xsd:element>
				<xsd:element ref="Title"  minOccurs="1" maxOccurs="unbounded"></xsd:element>
				<xsd:element ref="ChannelAgeLimit" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Age limit information about this serie.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="VersionableInfo" minOccurs="0" maxOccurs="1">
					<xsd:annotation><xsd:documentation>Versions of textual information about this serie.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="Version" minOccurs="1" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>One version of textual information about this serie.</xsd:documentation></xsd:annotation></xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
<!--				<xsd:element name="Image" type="ImageType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>DEPRECATED till förmån för MediaItem</xsd:documentation></xsd:annotation></xsd:element>-->
				<xsd:element ref="MediaItems" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Holder of media objects referenced from this serie.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="Description" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Textual information about this serie, valid for all seasons and episodes.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ExtraInfo" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Extra information.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element ref="ProgramData" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Data about the serie.</xsd:documentation></xsd:annotation></xsd:element>
			</xsd:sequence>
			<xsd:attributeGroup ref="grundAttributGrupp"/>
			<xsd:attribute name="MatchRank" type="xsd:integer"><xsd:annotation><xsd:documentation>Ranking for this serie in the registry matching</xsd:documentation></xsd:annotation></xsd:attribute>
			
		</xsd:complexType>
	</xsd:element>	
	<!-- ProgramSeries -->

		
<!-- Declarations of separate elements -->	

	<!-- Audio Language -->
	<xsd:element name="AudioLanguage">
		<xsd:annotation><xsd:documentation>Language in the main audio heard in the program.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="LanguageCode" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Code of the language heard</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="DubbedLanguageCode" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>If the program is dubbed, language code of the language it is dubbed in.</xsd:documentation></xsd:annotation></xsd:attribute>
			
		</xsd:complexType>
	</xsd:element>
	
	<!--  Channel-->
	<xsd:element name="Channel">
		<xsd:annotation><xsd:documentation>Channel name</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:string">
				     <xsd:attribute name="id" type="xsd:string" use="optional"></xsd:attribute>	
					<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal operations</xsd:documentation></xsd:annotation></xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>	

	<!-- ChannelAgeLimit -->
	<xsd:element name="ChannelAgeLimit">
		<xsd:annotation>
			<xsd:documentation>Information about age limits connedted with the channel.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:string">
					<xsd:attribute name="ChannelID" type="xsd:string" use="optional"></xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- ChannelRegistryReferenceID -->
	<xsd:element name="ChannelRegistryReferenceID"><xsd:annotation><xsd:documentation>Channel specific id of program.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:string">
					<xsd:attribute name="ChannelID" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>ID of the channel assigning this id.</xsd:documentation></xsd:annotation></xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- ! Description -->
	<xsd:element name="Description">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="TextDesc"  minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Textual description</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="LanguageCode" type="xsd:string" use="optional"></xsd:attribute>
								<xsd:attribute name="Part" type="xsd:string" use="required"><xsd:annotation><xsd:documentation>Can be one of three text parts</xsd:documentation></xsd:annotation></xsd:attribute>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Misc" minOccurs="0" >
					<xsd:annotation><xsd:documentation>Unused text and internal notes.</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Text" type="xsd:string" minOccurs="1" maxOccurs="unbounded"></xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>	
	<!-- Description -->
	
	<!-- ! ExtraInfo -->
	<xsd:element name="ExtraInfo">
		<xsd:annotation><xsd:documentation>Extra informationthat has no other place.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:string">
					<xsd:attribute name="InfoType" type="InfoTypeEnumList"></xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>			
	</xsd:element>		
	<!-- ExtraInfo -->
	
	<!-- ! Program Data-->
	<xsd:element name="ProgramData">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Category" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation><xsd:documentation>Content category of program</xsd:documentation></xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="xsd:string">
								<xsd:attribute name="id" type="xsd:string" use="optional"></xsd:attribute>
								<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal operation</xsd:documentation></xsd:annotation></xsd:attribute>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ProgramProduction" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Information about the production of the program.</xsd:documentation></xsd:annotation> 
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Year" type="xsd:string" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Year or years when the program was produced.</xsd:documentation></xsd:annotation></xsd:element>
							<xsd:element name="ProductionCompany" type="xsd:string" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Company/ies involved in the production.</xsd:documentation></xsd:annotation></xsd:element>
							<xsd:element name="CountryCode" type="xsd:string" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Country code(s) of the production.</xsd:documentation></xsd:annotation></xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>	
	<!-- Program Data -->
	
	
	<xsd:element name="Reviews">
		<xsd:annotation>
			<xsd:documentation>Review and/or ratings of the program.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Review" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>One review or rating.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Originator" minOccurs="0">
                               				<xsd:complexType>
                               					<xsd:sequence>
                               						<xsd:element name="Name" minOccurs="0" type="xsd:string"></xsd:element>
                               						<xsd:element name="OrganisationName" type="xsd:string" minOccurs="0"></xsd:element>
                               					</xsd:sequence>
                               					<xsd:attribute name="id" type="xsd:nonNegativeInteger" use="optional"><xsd:annotation><xsd:documentation>ID that can be used externally.</xsd:documentation></xsd:annotation></xsd:attribute>
                               					<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal handling</xsd:documentation></xsd:annotation></xsd:attribute>
                              				</xsd:complexType>								
							</xsd:element>
							<xsd:element name="RatingValue" minOccurs="0" type="xsd:string"/>
							<xsd:element name="ReviewTextShortHeadline" type="BaseTextType" minOccurs="0"/>
							<xsd:element name="ReviewTextLongHeadline" type="BaseTextType" minOccurs="0"/>
							<xsd:element name="ReviewTextShort" type="BaseTextType" minOccurs="0"/>
							<xsd:element name="ReviewTextLong" type="BaseTextType" minOccurs="0"/>
						</xsd:sequence>
<!--						<xsd:attribute name="CreatedDate" type="xsd:dateTime" use="optional"><xsd:annotation><xsd:documentation>DEPRECATED: Datum och klockslag då denna MediaItem skapades</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="ChangedDate" type="xsd:dateTime" use="optional"><xsd:annotation><xsd:documentation>DEPRECATED: Datum och klockslag då denna MediaItem senast ändrades.</xsd:documentation></xsd:annotation></xsd:attribute>
-->						<xsd:attribute name="id" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>ID that can be used externally.</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal handling</xsd:documentation></xsd:annotation></xsd:attribute>
					</xsd:complexType>
					
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>	
	
