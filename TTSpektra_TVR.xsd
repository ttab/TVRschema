		<xsd:attribute  name="id" type="xsd:string" use="optional"/>
		<xsd:attribute name="CreatedDate" type="xsd:dateTime" use="optional" ><xsd:annotation><xsd:documentation>Date and time of creation</xsd:documentation></xsd:annotation></xsd:attribute>
