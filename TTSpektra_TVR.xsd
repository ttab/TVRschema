		<xsd:attribute name="AccessStatus" type="xsd:nonNegativeInteger"  use="optional" ><xsd:annotation><xsd:documentation>Status of availability</xsd:documentation></xsd:annotation></xsd:attribute>
		<xsd:attribute name="RegistryReferenceId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Releationship to any program, season or serie in the registry.</xsd:documentation></xsd:annotation></xsd:attribute>
		<xsd:attribute name="ChangedBy" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Updated by.</xsd:documentation></xsd:annotation></xsd:attribute>
		<xsd:attribute name="CreatedBy" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Created by.</xsd:documentation></xsd:annotation></xsd:attribute>
		<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal handling.</xsd:documentation></xsd:annotation></xsd:attribute>
	</xsd:attributeGroup>
	
	
