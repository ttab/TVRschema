	<xsd:element name="MediaItems">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="MediaItem">
					<xsd:annotation>
						<xsd:documentation>Element to handle one MediaItem</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Copyright" minOccurs="0" maxOccurs="1">
								<xsd:annotation><xsd:documentation>Copyright information for this media item.</xsd:documentation></xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="CopyrightText" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Text explaining copyright information for this media item.</xsd:documentation></xsd:annotation></xsd:element>
										<xsd:element name="CopyrightOwner" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Holder of the copyright.</xsd:documentation></xsd:annotation></xsd:element>
										<xsd:element name="CopyrightOwnerName" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Name of the holder of this copyright.</xsd:documentation></xsd:annotation></xsd:element>
										<xsd:element name="CopyrightOwnerId" type="xsd:string" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Id of the holder of this copyright.</xsd:documentation></xsd:annotation></xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="DescriptiveMetadata" minOccurs="1" maxOccurs="1">
								<xsd:annotation><xsd:documentation>Själva informationen om detta MediaItem, filnamn osv.</xsd:documentation></xsd:annotation>
			   					<xsd:complexType>
			       					<xsd:sequence>
			       						<xsd:element name="Title" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Title of this media item.</xsd:documentation></xsd:annotation></xsd:element>
			       						<xsd:element name="Caption" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Description of this media item. Can be caption.</xsd:documentation></xsd:annotation></xsd:element>
			       						<xsd:element name="Creator" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Name of the creator of this media item.</xsd:documentation></xsd:annotation></xsd:element>
			       						<xsd:element name="Filename" type="xsd:string" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Filname suggested for this media item.</xsd:documentation></xsd:annotation></xsd:element>
			       						<xsd:element name="PublicURL" type="xsd:string" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Full URL to this media item.</xsd:documentation></xsd:annotation></xsd:element>
			       					</xsd:sequence>
			    				</xsd:complexType>		
							</xsd:element>
						</xsd:sequence>
						<xsd:attribute name="CreatedDate" type="xsd:dateTime" use="optional"><xsd:annotation><xsd:documentation>Date and time when the media item was created</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="ChangedDate" type="xsd:dateTime" use="optional"><xsd:annotation><xsd:documentation>Date and time when the media item was updated.</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="IsAutoCreated" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Internal note if it has been autocreated.</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="id" type="xsd:nonNegativeInteger" use="optional"><xsd:annotation><xsd:documentation>ID that can be used externally.</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal handling</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="Role" type="MediaItemRoleEnumList" use="optional"><xsd:annotation><xsd:documentation>Role this media item has.</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="Bytesize" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Size of the media item.</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="MimeType" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Mimetype for this MediaItem</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="MediaType" type="MediaItemTypeEnumList" use="optional"><xsd:annotation><xsd:documentation>Type of media item.</xsd:documentation></xsd:annotation></xsd:attribute>
						<xsd:attribute name="ForeignSystemId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>ID from the supplier of this media item.</xsd:documentation></xsd:annotation></xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				
			</xsd:sequence>
			<xsd:attribute name="CreatedDate" type="xsd:dateTime" use="optional"><xsd:annotation><xsd:documentation>Date and time when the media item was created</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="ChangedDate" type="xsd:dateTime" use="optional"><xsd:annotation><xsd:documentation>Date and time when the media item was updated.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="IsAutoCreated" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Internal note if it has been autocreated.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="id" type="xsd:nonNegativeInteger" use="optional"><xsd:annotation><xsd:documentation>ID that can be used externally.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal handling</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="Role" type="MediaItemRoleEnumList" use="optional"><xsd:annotation><xsd:documentation>Role this media item has.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="Bytesize" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Size of the media item.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="MimeType" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Mimetype for this MediaItem</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="MediaType" type="MediaItemTypeEnumList" use="optional"><xsd:annotation><xsd:documentation>Type of media item.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="ForeignSystemId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>ID from the supplier of this media item.</xsd:documentation></xsd:annotation></xsd:attribute>
		</xsd:complexType>
		
	</xsd:element>
	
	<!-- ! Sub_titles -->
	<xsd:element name="Subtitles">
		<xsd:annotation><xsd:documentation>Language on the subtitles of the program.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="LanguageCode" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Language code for the subtitles. </xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="Additional" type="xsd:boolean"><xsd:annotation><xsd:documentation></xsd:documentation></xsd:annotation></xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- Sub_titles -->
	
	<!-- ! Texttv_Subtitles -->
	<xsd:element name="TexttvSubtitles" type="BaseTextType"><xsd:annotation><xsd:documentation>If subtitles can be activated through text-tv.</xsd:documentation></xsd:annotation></xsd:element>
	<!-- Texttv_Subtitles -->
	
	<!-- Title -->
	<xsd:element name="Title"><xsd:annotation><xsd:documentation>The title for the program set by the channel.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:string">
					<xsd:attribute name="LanguageCode" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Language code for the title.</xsd:documentation></xsd:annotation></xsd:attribute>
					<xsd:attribute name="ChannelID" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>ID of the channel that sat this title.</xsd:documentation></xsd:annotation></xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Version av innehåll som kan vara olika för olika kunder -->
	<xsd:element name="Version">
		<xsd:annotation><xsd:documentation>One version of textual information about a program, a season or a serie.</xsd:documentation></xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="EpisodeTitle"  type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Specific title of an episode of a series.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="TTSTitle" type="BaseTextType"><xsd:annotation><xsd:documentation>TT title of program.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="Intro" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Short textline, like "Swedish documentary from 2015"</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="AltDescription" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Alternativ textual description.</xsd:documentation></xsd:annotation></xsd:element>
				<xsd:element name="Puff" type="BaseTextType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Text part used for sporting events</xsd:documentation></xsd:annotation></xsd:element>
			</xsd:sequence>
			<xsd:attribute  name="id" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>ID for this version of textual information.</xsd:documentation></xsd:annotation></xsd:attribute>
			<xsd:attribute name="InternalId" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>GUID for internal handling</xsd:documentation></xsd:annotation></xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
	
<!-- Types -->

		<xsd:complexType name="ImageType">
			<xsd:sequence>
				<xsd:element name="ImageFilename" type="xsd:string" minOccurs="1" maxOccurs="1">
					<xsd:annotation><xsd:documentation>Name of image file</xsd:documentation></xsd:annotation>
				</xsd:element>
				<xsd:element name="ImageTitle" type="xsd:string" minOccurs="0" maxOccurs="1">
					<xsd:annotation><xsd:documentation>Title of image</xsd:documentation></xsd:annotation>
				</xsd:element>
				<xsd:element name="ImageCreator" type="xsd:string" minOccurs="0" maxOccurs="1">
					<xsd:annotation><xsd:documentation>Name of the creator</xsd:documentation></xsd:annotation>
				</xsd:element>
				<xsd:element name="ImageCaption" type="BaseTextType" minOccurs="0" maxOccurs="1">
					<xsd:annotation><xsd:documentation>Image description, max 150 char.</xsd:documentation></xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	

	<!-- ! PersonListType -->
	<xsd:complexType name="PersonListType">
		<xsd:sequence>
			<xsd:element name="ProgramPerson" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Persons involved in this program.</xsd:documentation></xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Function" minOccurs="0" maxOccurs="1">
							<xsd:annotation><xsd:documentation>Function this program has in connection with this program.</xsd:documentation></xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="FunctionEnumList">
										<xsd:attribute name="Rank" type="xsd:nonNegativeInteger" use="optional"/><!-- DEPRECATED. The order in the xml is significant instead -->
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Name" type="xsd:string"><xsd:annotation><xsd:documentation>Name of the person.</xsd:documentation></xsd:annotation></xsd:element>
						<xsd:element name="Role" type="xsd:string" minOccurs="0"><xsd:annotation><xsd:documentation>If function is "Skådespelare", which means actor, this can hold the name of the character the actor play.</xsd:documentation></xsd:annotation></xsd:element>
					</xsd:sequence>
					<xsd:attribute name="RegistryReferenceId" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>	
		</xsd:sequence>
	</xsd:complexType>
	<!-- PersonListType -->
	
	<!-- Base_TExtType -->
	<xsd:annotation><xsd:documentation>Basic text part.</xsd:documentation></xsd:annotation>
	<xsd:complexType name="BaseTextType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="LanguageCode" type="xsd:string" use="optional"><xsd:annotation><xsd:documentation>Language code for the text.</xsd:documentation></xsd:annotation></xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- Base_TExtType -->
	
	
<!-- Attribut-deklarationer och Attribut-grupper -->	

	<xsd:attributeGroup name="grundAttributGrupp">
